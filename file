            {\"Scooter_ID\":\"SCOO165\",\"Zone_ID\":\"Z6\",\"Battery_Level\":66,\"Battery_Health\":67,\"Ride_Count\":12,\"Ride_Duration_Min\":29,\"Ride_Revenue\":19.7,\"Charge_Response_Time\":69,\"Charging_Duration\":144,\"Downtime_Minutes\":150,\"Is_Available\":1,\"Low_Battery_Flag\":0,\"Collection_Delay_Min\":111,\"Peak_Hour_Flag\":1,\"Weather_Condition\":\"Sunny\",\"Maintenance_Flag\":0,\"Day_of_Week\":\"Thursday\",\"Total_Daily_Revenue\":236.4,\"Distance_Traveled_KM\":10.9,\"Battery_Replacement_Required\":0},
            {\"Scooter_ID\":\"SCOO136\",\"Zone_ID\":\"Z7\",\"Battery_Level\":96,\"Battery_Health\":80,\"Ride_Count\":17,\"Ride_Duration_Min\":13,\"Ride_Revenue\":10.36,\"Charge_Response_Time\":49,\"Charging_Duration\":85,\"Downtime_Minutes\":104,\"Is_Available\":1,\"Low_Battery_Flag\":0,\"Collection_Delay_Min\":85,\"Peak_Hour_Flag\":1,\"Weather_Condition\":\"Sunny\",\"Maintenance_Flag\":0,\"Day_of_Week\":\"Saturday\",\"Total_Daily_Revenue\":176.12,\"Distance_Traveled_KM\":2.59,\"Battery_Replacement_Required\":0},
            {\"Scooter_ID\":\"SCOO149\",\"Zone_ID\":\"Z1\",\"Battery_Level\":25,\"Battery_Health\":75,\"Ride_Count\":13,\"Ride_Duration_Min\":40,\"Ride_Revenue\":23.49,\"Charge_Response_Time\":10,\"Charging_Duration\":107,\"Downtime_Minutes\":33,\"Is_Available\":1,\"Low_Battery_Flag\":0,\"Collection_Delay_Min\":53,\"Peak_Hour_Flag\":0,\"Weather_Condition\":\"Cloudy\",\"Maintenance_Flag\":0,\"Day_of_Week\":\"Saturday\",\"Total_Daily_Revenue\":305.37,\"Distance_Traveled_KM\":6.59,\"Battery_Replacement_Required\":0}
        ];

        // Global state
        let filteredData = [...rawData];
        let charts = {};

        // Initialize filters
        function initFilters() {
            const zones = [...new Set(rawData.map(d => d.Zone_ID))].sort();
            const days = ['Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday', 'Sunday'];
            const weather = [...new Set(rawData.map(d => d.Weather_Condition))].sort();

            const zoneFilter = document.getElementById('zoneFilter');
            zones.forEach(z => {
                const opt = document.createElement('option');
                opt.value = z;
                opt.textContent = z;
                zoneFilter.appendChild(opt);
            });

            const dayFilter = document.getElementById('dayFilter');
            days.forEach(d => {
                if (rawData.some(r => r.Day_of_Week === d)) {
                    const opt = document.createElement('option');
                    opt.value = d;
                    opt.textContent = d;
                    dayFilter.appendChild(opt);
                }
            });

            const weatherFilter = document.getElementById('weatherFilter');
            weather.forEach(w => {
                const opt = document.createElement('option');
                opt.value = w;
                opt.textContent = w;
                weatherFilter.appendChild(opt);
            });
        }

        // Apply filters
        function applyFilters() {
            const zone = document.getElementById('zoneFilter').value;
            const day = document.getElementById('dayFilter').value;
            const weather = document.getElementById('weatherFilter').value;
            const peak = document.getElementById('peakFilter').value;

            filteredData = rawData.filter(d => {
                if (zone !== 'all' && d.Zone_ID !== zone) return false;
                if (day !== 'all' && d.Day_of_Week !== day) return false;
                if (weather !== 'all' && d.Weather_Condition !== weather) return false;
                if (peak !== 'all' && d.Peak_Hour_Flag !== parseInt(peak)) return false;
                return true;
            });

            updateKPIs();
            updateAllCharts();
        }

        // Update KPIs
        function updateKPIs() {
            const totalDowntime = filteredData.reduce((s, d) => s + d.Downtime_Minutes, 0);
            const totalRevenue = filteredData.reduce((s, d) => s + d.Total_Daily_Revenue, 0);
            const avgBatteryHealth = filteredData.reduce((s, d) => s + d.Battery_Health, 0) / filteredData.length;
            const lowBatteryCount = filteredData.filter(d => d.Low_Battery_Flag === 1).length;
            const availabilityRate = (filteredData.filter(d => d.Is_Available === 1).length / filteredData.length * 100);

            document.getElementById('kpiGrid').innerHTML = `
                <div class=\"kpi-card\">
                    <div class=\"kpi-label\">Total Records</div>
                    <div class=\"kpi-value\">${filteredData.length}</div>
                    <div class=\"kpi-subtext\">scooter observations</div>
                </div>
                <div class=\"kpi-card info\">
                    <div class=\"kpi-label\">Total Revenue</div>
                    <div class=\"kpi-value\">$${totalRevenue.toLocaleString(undefined, {maximumFractionDigits: 0})}</div>
                    <div class=\"kpi-subtext\">daily revenue sum</div>
                </div>
                <div class=\"kpi-card warning\">
                    <div class=\"kpi-label\">Total Downtime</div>
                    <div class=\"kpi-value\">${(totalDowntime / 60).toFixed(0)}h</div>
                    <div class=\"kpi-subtext\">${totalDowntime.toLocaleString()} minutes</div>
                </div>
                <div class=\"kpi-card danger\">
                    <div class=\"kpi-label\">Low Battery Events</div>
                    <div class=\"kpi-value\">${lowBatteryCount}</div>
                    <div class=\"kpi-subtext\">${(lowBatteryCount / filteredData.length * 100).toFixed(1)}% of fleet</div>
                </div>
                <div class=\"kpi-card purple\">
                    <div class=\"kpi-label\">Avg Battery Health</div>
                    <div class=\"kpi-value\">${avgBatteryHealth.toFixed(0)}%</div>
                    <div class=\"kpi-subtext\">fleet average</div>
                </div>
                <div class=\"kpi-card\">
                    <div class=\"kpi-label\">Availability Rate</div>
                    <div class=\"kpi-value\">${availabilityRate.toFixed(1)}%</div>
                    <div class=\"kpi-subtext\">scooters available</div>
                </div>
            `;
        }

        // Chart 1: Downtime Attribution
        function createDowntimeChart() {
            const ctx = document.getElementById('downtimeChart').getContext('2d');
            
            const lowBatteryDowntime = filteredData
                .filter(d => d.Low_Battery_Flag === 1)
                .reduce((s, d) => s + d.Downtime_Minutes, 0);
            
            const maintenanceDowntime = filteredData
                .filter(d => d.Maintenance_Flag === 1)
                .reduce((s, d) => s + d.Downtime_Minutes, 0);
            
            const totalDowntime = filteredData.reduce((s, d) => s + d.Downtime_Minutes, 0);
            const otherDowntime = totalDowntime - lowBatteryDowntime - maintenanceDowntime;
            
            const lowBatteryPct = (lowBatteryDowntime / totalDowntime * 100).toFixed(1);
            const maintenancePct = (maintenanceDowntime / totalDowntime * 100).toFixed(1);
            const otherPct = (otherDowntime / totalDowntime * 100).toFixed(1);

            if (charts.downtime) charts.downtime.destroy();
            
            charts.downtime = new Chart(ctx, {
                type: 'doughnut',
                data: {
                    labels: ['Low Battery', 'Maintenance', 'Other Causes'],
                    datasets: [{
                        data: [lowBatteryDowntime, maintenanceDowntime, otherDowntime],
                        backgroundColor: ['#ef4444', '#f59e0b', '#64748b'],
                        borderColor: '#151c25',
                        borderWidth: 3,
                        hoverOffset: 8
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    cutout: '65%',
                    plugins: {
                        legend: {
                            position: 'right',
                            labels: {
                                color: '#94a3b8',
                                padding: 16,
                                font: { size: 12, family: 'IBM Plex Sans' },
                                usePointStyle: true,
                                pointStyle: 'circle'
                            }
                        },
                        tooltip: {
                            backgroundColor: '#1a222c',
                            titleColor: '#f1f5f9',
                            bodyColor: '#94a3b8',
                            borderColor: '#1e293b',
                            borderWidth: 1,
                            padding: 12,
                            callbacks: {
                                label: function(context) {
                                    const pct = (context.raw / totalDowntime * 100).toFixed(1);
                                    return `${context.label}: ${context.raw.toLocaleString()} min (${pct}%)`;
                                }
                            }
                        }
                    }
                }
            });

            document.getElementById('downtimeInsight').innerHTML = `
                <p><strong>${lowBatteryPct}%</strong> of total downtime (${lowBatteryDowntime.toLocaleString()} minutes) is directly caused by low battery events. 
                Maintenance contributes <strong>${maintenancePct}%</strong> (${maintenanceDowntime.toLocaleString()} min), while other operational factors account for ${otherPct}%.</p>
            `;
        }

        // Chart 2: Charging Response Time Impact
        function createResponseChart() {
            const ctx = document.getElementById('responseChart').getContext('2d');
            
            const scatterData = filteredData.map(d => ({
                x: d.Charge_Response_Time,
                y: d.Downtime_Minutes
            }));

            // Calculate correlation
            const n = filteredData.length;
            const sumX = filteredData.reduce((s, d) => s + d.Charge_Response_Time, 0);
            const sumY = filteredData.reduce((s, d) => s + d.Downtime_Minutes, 0);
            const sumXY = filteredData.reduce((s, d) => s + d.Charge_Response_Time * d.Downtime_Minutes, 0);
            const sumX2 = filteredData.reduce((s, d) => s + d.Charge_Response_Time ** 2, 0);
            const sumY2 = filteredData.reduce((s, d) => s + d.Downtime_Minutes ** 2, 0);
            
            const correlation = (n * sumXY - sumX * sumY) / 
                Math.sqrt((n * sumX2 - sumX ** 2) * (n * sumY2 - sumY ** 2));

            // Trendline
            const slope = (n * sumXY - sumX * sumY) / (n * sumX2 - sumX ** 2);
            const intercept = (sumY - slope * sumX) / n;

            if (charts.response) charts.response.destroy();
            
            charts.response = new Chart(ctx, {
                type: 'scatter',
                data: {
                    datasets: [{
                        label: 'Scooter Observations',
                        data: scatterData,
                        backgroundColor: 'rgba(14, 165, 233, 0.5)',
                        borderColor: '#0ea5e9',
                        borderWidth: 1,
                        pointRadius: 5,
                        pointHoverRadius: 8
                    }, {
                        label: 'Trend Line',
                        data: [
                            { x: 0, y: intercept },
                            { x: 200, y: slope * 200 + intercept }
                        ],
                        type: 'line',
                        borderColor: '#00d4aa',
                        borderWidth: 2,
                        borderDash: [5, 5],
                        pointRadius: 0,
                        fill: false
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    scales: {
                        x: {
                            title: {
                                display: true,
                                text: 'Charge Response Time (min)',
                                color: '#94a3b8',
                                font: { size: 12 }
                            },
                            grid: { color: '#1e293b' },
                            ticks: { color: '#64748b' }
                        },
                        y: {
                            title: {
                                display: true,
                                text: 'Downtime (min)',
                                color: '#94a3b8',
                                font: { size: 12 }
                            },
                            grid: { color: '#1e293b' },
                            ticks: { color: '#64748b' }
                        }
                    },
                    plugins: {
                        legend: {
                            labels: { color: '#94a3b8', usePointStyle: true }
                        },
                        tooltip: {
                            backgroundColor: '#1a222c',
                            titleColor: '#f1f5f9',
                            bodyColor: '#94a3b8',
                            borderColor: '#1e293b',
                            borderWidth: 1
                        }
                    }
                }
            });

            const corrStrength = Math.abs(correlation) > 0.5 ? 'strong' : Math.abs(correlation) > 0.3 ? 'moderate' : 'weak';
            const corrDirection = correlation > 0 ? 'positive' : 'negative';
            
            document.getElementById('responseInsight').innerHTML = `
                <p>Correlation coefficient: <strong>r = ${correlation.toFixed(3)}</strong> indicates a <strong>${corrStrength} ${corrDirection}</strong> relationship. 
                ${correlation > 0 ? 'Longer charging response times are associated with increased downtime.' : 'Faster response times correlate with reduced downtime.'} 
                The trend line suggests that every additional minute of response delay adds approximately <strong>${slope.toFixed(2)} minutes</strong> of downtime.</p>
            `;
        }

        // Chart 3: Zone-Level Battery Unavailability
        function createZoneChart() {
            const ctx = document.getElementById('zoneChart').getContext('2d');
            
            const zoneStats = {};
            filteredData.forEach(d => {
                if (!zoneStats[d.Zone_ID]) {
                    zoneStats[d.Zone_ID] = { total: 0, lowBattery: 0, unavailable: 0, downtime: 0 };
                }
                zoneStats[d.Zone_ID].total++;
                if (d.Low_Battery_Flag === 1) zoneStats[d.Zone_ID].lowBattery++;
                if (d.Is_Available === 0) zoneStats[d.Zone_ID].unavailable++;
                zoneStats[d.Zone_ID].downtime += d.Downtime_Minutes;
            });

            const zones = Object.keys(zoneStats).sort();
            const unavailabilityRates = zones.map(z => (zoneStats[z].unavailable / zoneStats[z].total * 100));
            const lowBatteryRates = zones.map(z => (zoneStats[z].lowBattery / zoneStats[z].total * 100));
            const avgDowntime = zones.map(z => zoneStats[z].downtime / zoneStats[z].total);

            if (charts.zone) charts.zone.destroy();
            
            charts.zone = new Chart(ctx, {
                type: 'bar',
                data: {
                    labels: zones,
                    datasets: [{
                        label: 'Unavailability Rate (%)',
                        data: unavailabilityRates,
                        backgroundColor: '#ef4444',
                        borderRadius: 4,
                        yAxisID: 'y'
                    }, {
                        label: 'Low Battery Rate (%)',
                        data: lowBatteryRates,
                        backgroundColor: '#f59e0b',
                        borderRadius: 4,
                        yAxisID: 'y'
                    }, {
                        label: 'Avg Downtime (min)',
                        data: avgDowntime,
                        type: 'line',
                        borderColor: '#00d4aa',
                        backgroundColor: 'rgba(0, 212, 170, 0.1)',
                        borderWidth: 2,
                        pointRadius: 4,
                        pointBackgroundColor: '#00d4aa',
                        fill: true,
                        yAxisID: 'y1'
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    scales: {
                        x: {
                            grid: { display: false },
                            ticks: { color: '#64748b' }
                        },
                        y: {
                            position: 'left',
                            title: {
                                display: true,
                                text: 'Rate (%)',
                                color: '#94a3b8'
                            },
                            grid: { color: '#1e293b' },
                            ticks: { color: '#64748b' }
                        },
                        y1: {
                            position: 'right',
                            title: {
                                display: true,
                                text: 'Avg Downtime (min)',
                                color: '#94a3b8'
                            },
                            grid: { display: false },
                            ticks: { color: '#64748b' }
                        }
                    },
                    plugins: {
                        legend: {
                            labels: { color: '#94a3b8', usePointStyle: true, padding: 16 }
                        },
                        tooltip: {
                            backgroundColor: '#1a222c',
                            titleColor: '#f1f5f9',
                            bodyColor: '#94a3b8',
                            borderColor: '#1e293b',
                            borderWidth: 1
                        }
                    }
                }
            });

            const worstZone = zones[unavailabilityRates.indexOf(Math.max(...unavailabilityRates))];
            const worstRate = Math.max(...unavailabilityRates).toFixed(1);
            
            document.getElementById('zoneInsight').innerHTML = `
                <p><strong>${worstZone}</strong> has the highest battery-related unavailability at <strong>${worstRate}%</strong>. 
                Zones with high unavailability rates combined with elevated low-battery incidents should be prioritized for charging infrastructure improvements and technician deployment optimization.</p>
            `;
        }

        // Chart 4: Battery Health vs Maintenance
        function createHealthChart() {
            const ctx = document.getElementById('healthChart').getContext('2d');
            
            const healthBuckets = {
                'Critical (<60%)': { maintenance: 0, noMaintenance: 0, downtimeSum: 0, count: 0 },
                'Fair (60-75%)': { maintenance: 0, noMaintenance: 0, downtimeSum: 0, count: 0 },
                'Good (75-90%)': { maintenance: 0, noMaintenance: 0, downtimeSum: 0, count: 0 },
                'Excellent (>90%)': { maintenance: 0, noMaintenance: 0, downtimeSum: 0, count: 0 }
            };

            filteredData.forEach(d => {
                let bucket;
                if (d.Battery_Health < 60) bucket = 'Critical (<60%)';
                else if (d.Battery_Health < 75) bucket = 'Fair (60-75%)';
                else if (d.Battery_Health < 90) bucket = 'Good (75-90%)';
                else bucket = 'Excellent (>90%)';

                healthBuckets[bucket].count++;
                healthBuckets[bucket].downtimeSum += d.Downtime_Minutes;
                if (d.Maintenance_Flag === 1) {
                    healthBuckets[bucket].maintenance++;
                } else {
                    healthBuckets[bucket].noMaintenance++;
                }
            });

            const labels = Object.keys(healthBuckets);
            const maintenanceRates = labels.map(l => 
                healthBuckets[l].count > 0 ? (healthBuckets[l].maintenance / healthBuckets[l].count * 100) : 0
            );
            const avgDowntime = labels.map(l => 
                healthBuckets[l].count > 0 ? (healthBuckets[l].downtimeSum / healthBuckets[l].count) : 0
            );

            if (charts.health) charts.health.destroy();
            
            charts.health = new Chart(ctx, {
                type: 'bar',
                data: {
                    labels: labels,
                    datasets: [{
                        label: 'Maintenance Rate (%)',
                        data: maintenanceRates,
                        backgroundColor: ['#ef4444', '#f59e0b', '#0ea5e9', '#00d4aa'],
                        borderRadius: 6,
                        barPercentage: 0.7
                    }]
                },
                options: {
                    indexAxis: 'y',
                    responsive: true,
                    maintainAspectRatio: false,
                    scales: {
                        x: {
                            title: {
                                display: true,
                                text: 'Maintenance Occurrence Rate (%)',
                                color: '#94a3b8'
                            },
                            grid: { color: '#1e293b' },
                            ticks: { color: '#64748b' }
                        },
                        y: {
                            grid: { display: false },
                            ticks: { color: '#94a3b8', font: { size: 11 } }
                        }
                    },
                    plugins: {
                        legend: { display: false },
                        tooltip: {
                            backgroundColor: '#1a222c',
                            titleColor: '#f1f5f9',
                            bodyColor: '#94a3b8',
                            borderColor: '#1e293b',
                            borderWidth: 1,
                            callbacks: {
                                afterLabel: function(context) {
                                    const idx = context.dataIndex;
                                    return `Avg Downtime: ${avgDowntime[idx].toFixed(0)} min`;
                                }
                            }
                        }
                    }
                }
            });

            const criticalRate = maintenanceRates[0].toFixed(1);
            const excellentRate = maintenanceRates[3].toFixed(1);
            
            document.getElementById('healthInsight').innerHTML = `
                <p>Scooters with <strong>critical battery health (&lt;60%)</strong> show a maintenance rate of <strong>${criticalRate}%</strong>, compared to only <strong>${excellentRate}%</strong> for excellent condition batteries. 
                This ${(maintenanceRates[0] / (maintenanceRates[3] || 1)).toFixed(1)}x difference demonstrates the strong correlation between battery degradation and maintenance requirements.</p>
            `;
        }

        // Chart 5: Peak Hour Downtime vs Revenue
        function createRevenueChart() {
            const ctx = document.getElementById('revenueChart').getContext('2d');
            
            const peakData = filteredData.filter(d => d.Peak_Hour_Flag === 1);
            const offPeakData = filteredData.filter(d => d.Peak_Hour_Flag === 0);

            // Create downtime buckets
            const buckets = ['0-50', '51-100', '101-150', '151-200', '200+'];
            
            function getBucket(downtime) {
                if (downtime <= 50) return 0;
                if (downtime <= 100) return 1;
                if (downtime <= 150) return 2;
                if (downtime <= 200) return 3;
                return 4;
            }

            const peakRevenue = [0, 0, 0, 0, 0];
            const peakCount = [0, 0, 0, 0, 0];
            const offPeakRevenue = [0, 0, 0, 0, 0];
            const offPeakCount = [0, 0, 0, 0, 0];

            peakData.forEach(d => {
                const idx = getBucket(d.Downtime_Minutes);
                peakRevenue[idx] += d.Total_Daily_Revenue;
                peakCount[idx]++;
            });

            offPeakData.forEach(d => {
                const idx = getBucket(d.Downtime_Minutes);
                offPeakRevenue[idx] += d.Total_Daily_Revenue;
                offPeakCount[idx]++;
            });

            const avgPeakRevenue = peakRevenue.map((r, i) => peakCount[i] > 0 ? r / peakCount[i] : 0);
            const avgOffPeakRevenue = offPeakRevenue.map((r, i) => offPeakCount[i] > 0 ? r / offPeakCount[i] : 0);

            if (charts.revenue) charts.revenue.destroy();
            
            charts.revenue = new Chart(ctx, {
                type: 'line',
                data: {
                    labels: buckets.map(b => b + ' min'),
                    datasets: [{
                        label: 'Peak Hours Avg Revenue',
                        data: avgPeakRevenue,
                        borderColor: '#00d4aa',
                        backgroundColor: 'rgba(0, 212, 170, 0.15)',
                        borderWidth: 3,
                        pointRadius: 6,
                        pointBackgroundColor: '#00d4aa',
                        pointBorderColor: '#151c25',
                        pointBorderWidth: 2,
                        fill: true,
                        tension: 0.3
                    }, {
                        label: 'Off-Peak Avg Revenue',
                        data: avgOffPeakRevenue,
                        borderColor: '#64748b',
                        backgroundColor: 'rgba(100, 116, 139, 0.1)',
                        borderWidth: 3,
                        pointRadius: 6,
                        pointBackgroundColor: '#64748b',
                        pointBorderColor: '#151c25',
                        pointBorderWidth: 2,
                        fill: true,
                        tension: 0.3
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    scales: {
                        x: {
                            title: {
                                display: true,
                                text: 'Downtime Bucket',
                                color: '#94a3b8',
                                font: { size: 12 }
                            },
                            grid: { color: '#1e293b' },
                            ticks: { color: '#64748b' }
                        },
                        y: {
                            title: {
                                display: true,
                                text: 'Average Daily Revenue ($)',
                                color: '#94a3b8',
                                font: { size: 12 }
                            },
                            grid: { color: '#1e293b' },
                            ticks: { 
                                color: '#64748b',
                                callback: value => '$' + value.toFixed(0)
                            }
                        }
                    },
                    plugins: {
                        legend: {
                            labels: { color: '#94a3b8', usePointStyle: true, padding: 20 }
                        },
                        tooltip: {
                            backgroundColor: '#1a222c',
                            titleColor: '#f1f5f9',
                            bodyColor: '#94a3b8',
                            borderColor: '#1e293b',
                            borderWidth: 1,
                            callbacks: {
                                label: function(context) {
                                    return context.dataset.label + ': $' + context.raw.toFixed(2);
                                }
                            }
                        }
                    }
                }
            });

            const peakLowDowntime = avgPeakRevenue[0];
            const peakHighDowntime = avgPeakRevenue[4];
            const revenueLoss = peakLowDowntime - peakHighDowntime;
            
            document.getElementById('revenueInsight').innerHTML = `
                <p>During <strong>peak hours</strong>, scooters with minimal downtime (0-50 min) generate an average of <strong>$${peakLowDowntime.toFixed(2)}</strong> daily, 
                while those with high downtime (200+ min) average only <strong>$${peakHighDowntime.toFixed(2)}</strong>. 
                This represents a potential revenue loss of <strong>$${revenueLoss.toFixed(2)}</strong> per scooter per day during peak demand periods.</p>
            `;
        }

        // Update all charts
        function updateAllCharts() {
            createDowntimeChart();
            createResponseChart();
            createZoneChart();
            createHealthChart();
            createRevenueChart();
        }

        // Initialize dashboard
        document.addEventListener('DOMContentLoaded', function() {
            initFilters();
            updateKPIs();
            updateAllCharts();
        });
    </script>
</body>
</html>
"
